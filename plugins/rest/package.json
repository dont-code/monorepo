{
  "name": "@dontcode/plugin-rest",
  "version": "1.7.2",
  "license": "MIT",
  "main": "dist/libs/rest/esm2020/index.mjs",
  "typings": "dist/libs/rest/index.d.ts",
  "files": ["dist/libs/rest"],
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "build": "nx run rest:build:development",
    "build-for": "nx run rest:build:${npm_config_build}",
    "build-deploy": "nx run rest-tester:build:${npm_config_build} --output-path=\"dist/apps/rest-tester/${npm_config_stage}\" --base-href=\"https://run.dont-code.net/plugin-rest/${npm_config_stage}\"", 
    "clean": "rm -rf dist/*",
    "test": "nx test",
    "align-version": "PACKAGE_VERSION=$(npm view @dontcode/plugin-rest versions --json | jq -r '.[-1]') && cd libs/rest && npm version $PACKAGE_VERSION --allow-same-version",
    "push-all": "cd libs/rest && npm version prerelease && cd ../.. && nx run rest:build:development && cd dist/libs/rest && yalc push",
    "push": "cd libs/${npm_config_dir} && npm version prerelease && cd ../.. && nx run ${npm_config_dir}:build:development && cd dist/libs/${npm_config_dir} && yalc push"
  },
  "private": false,
